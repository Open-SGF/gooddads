{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "start": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["@repo/supabase#start"]
    },
    "@repo/supabase#start": {
      "outputs": [],
      "cache": false
    },
    "@repo/supabase#stop": {
      "outputs": [],
      "cache": false
    },
    "@repo/supabase#reset": {
      "outputs": [],
      "cache": false
    },
    "@repo/supabase#status": {
      "outputs": [],
      "cache": false
    },
    "@repo/supabase#generate": {
      "outputs": [],
      "cache": false
    }
  }
}
